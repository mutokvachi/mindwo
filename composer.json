{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.19",
        "laravel/framework": "5.2.*",
        "mindwo/pages" : "^1.5",
        "doctrine/dbal": "dev-master",
        "imagine/imagine": "dev-master",
        "illuminate/html": "dev-master",
        "phpdocumentor/phpdocumentor": "2.*",
        "guzzlehttp/guzzle": "^6.1",
        "intervention/image": "^2.3",
        "mobiledetect/mobiledetectlib": "^2.8",
        "asika/pdf2text": "^1.0",
        "spatie/laravel-backup": "^3.8",
        "maatwebsite/excel": "^2.1",
        "barryvdh/laravel-snappy": "^0.3.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3",
        "mariuzzo/laravel-js-localization": "^1.3",
        "adldap2/adldap2": "6.1.*",
        "phpoffice/phpword": "v0.13.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "webpatser/laravel-uuid": "dev-master",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Libraries"
        ],
        "psr-0": {
            "Pqb\\FilemanagerLaravel\\": "app/Packages/filemanager-laravel/src"
        },
        "psr-4": {
            "App\\": "app/",
            "mindwo\\pages\\": "vendor/mindwo/pages/src",
            "PhpOffice\\PhpWord\\": "app/Packages/phpword/src/PhpWord"
        },
        "files": [
            "app/Exceptions/DXExceptions.php",
            "vendor/mobiledetect/mobiledetectlib/namespaced/Detection/MobileDetect.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('config/app.example.php', 'config/app.php');\"",
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php -r \"copy('config/app.example.php', 'config/app.php');\"",
            "git -C vendor/mindwo/pages pull origin master",
            "php artisan vendor:publish --tag=setup --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mindwo/pages",
                "version": "1.5",
                "source": {
                    "url": "https://doitex@bitbucket.org/doitex_team/mindwopackages.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "autoload": {
                    "psr-0" : {
                        "mindwo\\pages" : "src"
                    }
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
